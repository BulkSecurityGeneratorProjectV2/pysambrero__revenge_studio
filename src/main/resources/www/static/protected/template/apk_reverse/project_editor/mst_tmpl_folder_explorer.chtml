{$mst_if_start}data.elements.0}}
<div class="row">
    <div class="col-xs-12">
        <div>
            {$mst_if_start}data.elements}}

            {$mst_if_start}thumb_b64}}
            <div class="exp-img-cont" title="{{title}}" data-node-id="{{nodeId}}" data-node-label="{{label}}" data-node-type="{{mimeCategory}}">

                <div>
                    <img class="exp-img-icon" src="{{thumb_link}}"/>
                </div>
                <div class="text-overflow">
                    <span class="exp-img-name">{{name}}</span>
                </div>
            </div>
            {{/thumb_link}}
            {{/thumb_b64}}


            {{/data.elements}}
        </div>

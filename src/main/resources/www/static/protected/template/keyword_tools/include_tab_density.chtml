<div class="col-xs-12">

    <form id="kw-density-form" name="kw-density-form">
        <fieldset>
            <div class="form-group">

                <textarea rows="8" style="width: 100%; padding: 10px; resize: vertical;"
                          id="kw-textarea"
                          name="kw-textarea"
                          class="max-length form-control"
                          placeholder="paste text here...(only english is supported)"
                          autocomplete="off" maxlength="4000"></textarea>
            </div>

            <input type="button" value="Calculate keyword density"
                   name="keyworddensitytool"
                   class="btn btn-success btn-sm" style="margin-bottom: 15px;" onclick="KeywordToolsModule.displayDensity();">
            <input type="button" value="Reset" class="btn btn-danger btn-sm" style="margin-bottom: 15px;"
                   onclick="KeywordToolsModule.resetDensityUi();">
        </fieldset>
    </form>


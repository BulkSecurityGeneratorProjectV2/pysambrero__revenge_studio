    <div class="form-group">
        <!--<div id="kw-density-results">-->
        <h2>Density result</h2>

        <div class="row">

            <div class="col-xs-12 col-sm-4">
                <div class="panel panel-sexy">
                    <div class="panel-heading">One word</div>
                    <div class="panel-body">
                        <ul id="kw-density-results-one" class="density-list list-unstyled"></ul>
                    </div>
                </div>
            </div>

            <div class="col-xs-12 col-sm-4">
                <div class="panel panel-sexy">
                    <div class="panel-heading">Two words</div>
                    <div class="panel-body">
                        <ul id="kw-density-results-two" class="density-list list-unstyled"></ul>
                    </div>
                </div>
            </div>

            <div class="col-xs-12 col-sm-4">
                <div class="panel panel-sexy">
                    <div class="panel-heading">Three words</div>
                    <div class="panel-body">
                        <ul id="kw-density-results-three" class="density-list list-unstyled"></ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
